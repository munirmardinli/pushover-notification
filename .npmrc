prefer-offline=true
prefer-dedupe=true           # Vermeidet doppelte Pakete

legacy-peer-deps=true
strict-peer-deps=true
package-lock=true
global-style=true 

strict-ssl=true
maxsockets=50
fetch-retries=3
fetch-retry-mintimeout=1000
fetch-retry-maxtimeout=60000
save-exact=true 

loglevel=warn
progress=true
timing=true
color=true
fund=false

audit=false
ignore-scripts=false
